<h2>games</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Spools.Models.Game.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//spools/views/game/list',{games: games})%>
	</tbody>
</table>
<h2>New game</h2>
<form>
	<% for(var attr in Spools.Models.Game.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>